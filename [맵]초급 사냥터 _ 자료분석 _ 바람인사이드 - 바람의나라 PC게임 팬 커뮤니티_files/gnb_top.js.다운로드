var layer_home="메인으로";var layer_se="사이트 검색";var layer_commu="내소식알림";jQuery(function(){jQuery(".link li[id^=btn]").hover(function(){var el=jQuery(this).attr('id').split("_")[1];var str=eval("layer_"+el);jQuery(this).append("<div class='top_layer slogan_"+el+"'><span class='ico_up_dgray'></span><p>"+str+"</p></div>");},function(){jQuery(".link li[id^=btn] .top_layer").remove();});});